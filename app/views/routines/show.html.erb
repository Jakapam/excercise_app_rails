<h2><%= @routine.name %></h2><br>
<% @exercises.each do |exercise| %>
<h3>- - - </h3>
<p>Exercise Name:<%= exercise.exercise.name %></p>
<p>Sets:<%= exercise.sets %></p>
<p> Reps:<%= exercise.reps %></p>
<p> Weight:<%= exercise.weight %></p>

<% end %>

<% if @routine.user_id == session[:user_id] %>
  <%= link_to 'Add Exercise to this Routine', new_routine_routine_exercise_path(@routine) %>
<% end %>
